{
  "name": "yume",
  "displayName": "Yume",
  "description": "A language made by a 13 year old)",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "yume",
        "aliases": [
          "Yume",
          "yume"
        ],
        "extensions": [
          ".yum"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "yume",
        "scopeName": "source.yume",
        "path": "./syntaxes/yume.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vandi37/Yume-pack"
  },
  "devDependencies": {
    "esbuild": "^0.25.1"
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
