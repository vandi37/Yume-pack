{
	"name": "My Language",
	"scopeName": "source.yume",
	"fileTypes": [
		"yum"
	],
	"patterns": [
		{
			"name": "comment.line.double-exclamation.yume",
			"match": "!!.*$"
		},
		{
			"name": "comment.block.yume",
			"begin": "!/",
			"end": "/!",
			"patterns": [
				{
					"include": "#comment-content"
				}
			]
		},
		{
			"name": "string.quoted.double.yume",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.yume",
					"match": "\\\\."
				}
			]
		},
		{
			"name": "string.quoted.template.yume",
			"begin": "`",
			"end": "`",
			"patterns": [
				{
					"name": "constant.character.escape.yume",
					"match": "\\\\."
				}
			]
		},
		{
			"name": "string.quoted.single.yume",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.yume",
					"match": "\\\\."
				}
			]
		},
		{
			"name": "keyword.operator.arithmetic.yume",
			"match": "\\+|\\-|\\/|\\*|\\%"
		},
		{
			"name": "keyword.operator.bitwise.yume",
			"match": "&|\\||\\^|~|<<|>>"
		},
		{
			"name": "keyword.operator.logical.yume",
			"match": "&&|\\|\\||!"
		},
		{
			"name": "keyword.operator.relational.yume",
			"match": "!=|=="
		},
		{
			"name": "keyword.operator.assignment.yume",
			"match": "=|\\+=|\\-=|\\*=|\\/=|\\%=|\\+\\+|\\-\\-"
		},
		{
			"name": "keyword.operator.misc.yume",
			"match": "@"
		},
		{
			"name": "constant.numeric.decimal.yume",
			"match": "\\b\\d+(\\.\\d+)?\\b"
		},
		{
			"name": "constant.numeric.binary.yume",
			"match": "\\b0b[01]+(\\.[01]+)?\\b"
		},
		{
			"name": "constant.numeric.octal.yume",
			"match": "\\b0o[0-7]+(\\.[0-7]+)?\\b"
		},
		{
			"name": "constant.numeric.hexadecimal.yume",
			"match": "\\b0x[0-9a-fA-F]+(\\.[0-9a-fA-F]+)?\\b"
		},
		
		{
			"name": "keyword.enum.yume",
			"match": "\\benum\\b"
		},
		{
			"name": "keyword.proc.yume",
			"match": "\\bproc\\b"
		},
		{
			"name": "keyword.object.yume",
			"match": "\\bobject\\b"
		},
		{
			"name": "keyword.struct.yume",
			"match": "\\bstruct\\b"
		},
		{
			"name": "keyword.abstract.yume",
			"match": "\\babstract\\b"
		},
		{
			"name": "keyword.modifier.dec.yume",
			"match": "\\bdec\\b"
		},
		{
			"name": "keyword.modifier.const.yume",
			"match": "\\bconst\\b"
		},
		{
			"name": "keyword.modifier.as.yume",
			"match": "\\bas\\b"
		},
		{
			"name": "keyword.modifier.default.yume",
			"match": "\\bdefault\\b"
		},
		{
			"name": "keyword.modifier.when.yume",
			"match": "\\bwhen\\b"
		},
		{
			"name": "keyword.modifier.fail.yume",
			"match": "\\bfail\\b"
		},
		{
			"name": "keyword.modifier.adapt.yume",
			"match": "\\badapt\\b"
		},
		{
			"name": "keyword.modifier.to.yume",
			"match": "\\bto\\b"
		},
		{
			"name": "keyword.hoshi.yume",
			"match": "\\bhoshi\\b"
		},
		{
			"name": "keyword.modifier.zero.yume",
			"match": "\\bzero\\b"
		},
		{
			"name": "constant.language.null.yume",
			"match": "\\bnull\\b"
		},
		{
			"name": "keyword.modifier.void.yume",
			"match": "\\bvoid\\b"
		},
		{
			"name": "entity.name.tag.self.yume",
			"match": "\\bself\\b"
		},
		{
			"name": "keyword.control.if.yume",
			"match": "\\bif\\b"
		},
		{
			"name": "keyword.control.else.yume",
			"match": "\\belse\\b"
		},
		{
			"name": "keyword.control.while.yume",
			"match": "\\bwhile\\b"
		},
		{
			"name": "keyword.control.repeat.yume",
			"match": "\\brepeat\\b"
		},
		{
			"name": "keyword.control.for.yume",
			"match": "\\bfor\\b"
		},
		
		{
			"name": "keyword.control.in.yume",
			"match": "\\bin\\b"
		},
		{
			"name": "keyword.control.break.yume",
			"match": "\\bbreak\\b"
		},
		{
			"name": "keyword.control.continue.yume",
			"match": "\\bcontinue\\b"
		},
		{
			"name": "keyword.control.return.yume",
			"match": "\\breturn\\b"
		},
		
		{
			"name": "keyword.control.finally.yume",
			"match": "\\bfinally\\b"
		},
		{
			"name": "keyword.control.exit.yume",
			"match": "\\bexit\\b"
		},
		
		{
			"name": "keyword.control.panic.yume",
			"match": "\\bpanic\\b"
		},
		{
			"name": "keyword.control.try.yume",
			"match": "\\btry\\b"
		},
		{
			"name": "entity.name.type.int.yume",
			"match": "\\bint\\b"
		},
		{
			"name": "entity.name.type.uint.yume",
			"match": "\\buint\\b"
		},
		
		{
			"name": "entity.name.type.byte.yume",
			"match": "\\bbyte\\b"
		},
		{
			"name": "entity.name.type.char.yume",
			"match": "\\bchar\\b"
		},
		{
			"name": "entity.name.type.string.yume",
			"match": "\\bstring\\b"
		},
		{
			"name": "entity.name.type.option.yume",
			"match": "\\boption\\b"
		},
		{
			"name": "entity.name.type.promise.yume",
			"match": "\\bpromise\\b"
		},
		{
			"name": "constant.language.true.yume",
			"match": "\\btrue\\b"
		},
		{
			"name": "constant.language.false.yume",
			"match": "\\bfalse\\b"
		},
		{
			"name": "entity.name.type.any.yume",
			"match": "\\bany\\b"
		},
		{
			"name": "entity.name.type.error.yume",
			"match": "\\berror\\b"
		},
		
		{
			"name": "entity.name.type.equal.yume",
			"match": "\\bequal\\b"
		},

		
		{
			"name": "entity.name.type.not.yume",
			"match": "\\bnot\\b"
		},
		
		{
			"name": "entity.name.type.compare.yume",
			"match": "\\bcompare\\b"
		},
		{
			"name": "entity.name.type.float.yume",
			"match": "\\bfloat\\b"
		},
		{
			"name": "entity.name.type.bool.yume",
			"match": "\\bbool\\b"
		},
		{
			"name": "entity.name.type.stringer.yume",
			"match": "\\bstringer\\b"
		},
		{
			"name": "support.type.import.yume",
			"match": "([A-Za-z_]([A-Za-z0-9_]+)?)(\\.\\.)",
			"captures": {
				"1": {
					"name": "support.type.import.yume"
				}
			}
		},
		{
			"name": "entity.name.function.call.yume",
			"match": "([A-Za-z_]([A-Za-z0-9_]+)?)\\(",
			"captures": {
				"1": {
					"name": "entity.name.function.call.yume"
				}
			}
		},
		{
			"name": "variable.parameter.other.yume",
			"match": "\\b[A-Za-z_]([A-Za-z0-9_]+)?\\b"
		},
		{
			"name": "keyword.operator.colon.yume",
			"match": ":"
		},
		{
			"name": "keyword.operator.dot.yume",
			"match": "."
		},
		
		{
			"name": "keyword.operator.question.mark.yume",
			"match": "\\?"
		}
	],
	"repository": {
		"comment-content": {
			"patterns": []
		}
	}
}